<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="access" />
		<Option execution_dir="../minimal" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="win_gcc">
				<Option output="bin/$(TARGET_NAME)/$(PROJECT_NAME)" prefix_auto="1" extension_auto="1" />
				<Option object_output=".objs/$(TARGET_NAME)" />
				<Option type="0" />
				<Option compiler="gcc" />
				<Option projectLinkerOptionsRelation="2" />
				<Compiler>
					<Add option="-O2" />
					<Add option="`$(WX_CONFIG) --cflags`" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add option="`$(WX_CONFIG) --libs base,core`" />
					<Add directory="$(WXWIN)/lib/$(WXCFG)" />
				</Linker>
				<Environment>
					<Variable name="WX_CONFIG" value="wx-config --compiler=gcc --wxcfg=$(WXCFG)" />
				</Environment>
			</Target>
			<Target title="msys2_gcc">
				<Option output="bin/$(TARGET_NAME)/$(PROJECT_NAME)" prefix_auto="1" extension_auto="1" />
				<Option object_output=".objs/$(TARGET_NAME)" />
				<Option type="0" />
				<Option compiler="gcc" />
				<Option projectLinkerOptionsRelation="2" />
				<Compiler>
					<Add option="-O2" />
					<Add option="`$(WX_CONFIG) --cflags`" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add option="`$(WX_CONFIG) --libs base,core`" />
				</Linker>
				<Environment>
					<Variable name="WX_CONFIG" value="wx-config.exe  --shell=$(TARGET_COMPILER_DIR)..\usr\bin\sh.exe --wxconfig=$(TARGET_COMPILER_DIR)..\usr\local\bin\wx-config" />
				</Environment>
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="win_gcc;" />
		</VirtualTargets>
		<Unit filename="accesstest.cpp" />
		<Extensions>
			<code_completion />
			<envvars />
		</Extensions>
	</Project>
</CodeBlocks_project_file>

<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="console" />
		<Option execution_dir="../../console" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="win_gcc">
				<Option output="bin/$(TARGET_NAME)/$(PROJECT_NAME)" prefix_auto="1" extension_auto="1" />
				<Option working_dir="../../console" />
				<Option object_output=".objs/$(TARGET_NAME)" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Option projectLinkerOptionsRelation="2" />
				<Compiler>
					<Add option="-O2" />
					<Add option="`$(WX_CONFIG) --cflags`" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add option="`$(WX_CONFIG) --libs base`" />
					<Add directory="$(WXWIN)/lib/$(WXCFG)" />
				</Linker>
				<Environment>
					<Variable name="WX_CONFIG" value="wx-config --compiler=gcc --wxcfg=$(WXCFG)" />
				</Environment>
			</Target>
			<Target title="msys2_gcc">
				<Option output="bin/$(TARGET_NAME)/$(PROJECT_NAME)" prefix_auto="1" extension_auto="1" />
				<Option working_dir="../../console" />
				<Option object_output=".objs/$(TARGET_NAME)" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Option projectLinkerOptionsRelation="2" />
				<Compiler>
					<Add option="-O2" />
					<Add option="`$(WX_CONFIG) --cflags`" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add option="`$(WX_CONFIG) --libs base`" />
				</Linker>
				<Environment>
					<Variable name="WX_CONFIG" value="wx-config.exe  --prefix=$(TARGET_COMPILER_DIR)" />
				</Environment>
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="win_gcc;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-DwxUSE_GUI=0" />
		</Compiler>
		<ExtraCommands>
			<Add before="CMD /C echo $(WXWIN)/lib/$(WXCFG)" />
			<Add before="CMD /C $(WX_CONFIG) --cflags" />
		</ExtraCommands>
		<Unit filename="console.cpp" />
		<Extensions>
			<code_completion />
			<envvars />
		</Extensions>
	</Project>
</CodeBlocks_project_file>

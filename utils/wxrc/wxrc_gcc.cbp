				<Option object_output="gcc_mswud/wxrc" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-Wall" />
					<Add option="`$(WX_CONFIG) --cflags --debug=yes --static=yes`" />
					<Add option="-DWIN32" />
					<Add option="-D_DEBUG" />
					<Add option="-D__WXMSW__" />
					<Add option="-D_UNICODE" />

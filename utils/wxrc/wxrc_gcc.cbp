					<Add directory="." />
				</Compiler>
				<ResourceCompiler>
					<Add directory="./../../include" />
					<Add directory="." />
				</ResourceCompiler>
				<Linker>
					<Add option="-s" />
					<Add option="`$(WX_CONFIG) --libs base,xml`" />
					<Add library="kernel32" />
					<Add library="user32" />

					<Add library="wsock32" />
					<Add library="wininet" />
				</Linker>
				<Environment>
					<Variable name="WX_CONFIG" value="wx-config --compiler=gcc --wxcfg=$(WXCFG) --debug=yes --static=yes" />
				</Environment>
			</Target>
